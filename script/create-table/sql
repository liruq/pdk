-- 导出  表 book.pdk_sm_user 结构
CREATE TABLE IF NOT EXISTS `pdk_sm_user` (
  `id` char(24) NOT NULL,
  `name` varchar(50) CHARACTER SET utf8mb4 DEFAULT NULL,
  `type` smallint(6) DEFAULT NULL,
  `real_name` varchar(50) DEFAULT NULL,
  `sex` smallint(6) DEFAULT NULL,
  `age` smallint(6) DEFAULT NULL,
  `phone` varchar(11) DEFAULT '',
  `register_time` datetime DEFAULT NULL,
  `un_register_time` datetime DEFAULT NULL,
  `header_img` varchar(200) DEFAULT NULL,
  `status` smallint(6) DEFAULT NULL,
  `memo` varchar(400) DEFAULT NULL,
  `ts` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `dr` smallint(6) DEFAULT '0',
  `address` varchar(255) DEFAULT NULL,
  `source_id` varchar(50) DEFAULT NULL,
  `event_key` varchar(100) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `source_id` (`source_id`) USING BTREE
) ENGINE=MyISAM DEFAULT CHARSET=utf8;

