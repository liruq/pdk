CREATE TABLE IF NOT EXISTS `pdk_chat_chatmsg` (
  `id` char(24) NOT NULL,
  `from_id` char(24) DEFAULT NULL,
  `from_name` varchar(50) DEFAULT NULL,
  `send_to_id` char(24) DEFAULT NULL,
  `send_to_name` char(24) DEFAULT NULL,
  `msg_type` smallint(6) DEFAULT NULL,
  `create_time` datetime DEFAULT NULL,
  `content` varchar(400) DEFAULT NULL,
  `source` smallint(6) DEFAULT NULL,
  `ts` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `dr` smallint(6) DEFAULT '0',
  `from_head_img` varchar(200) DEFAULT NULL,
  `send_to_head_img` varchar(200) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `I_chat_chatmsg_1` (`from_id`) USING BTREE,
  KEY `I_chat_chatmsg_2` (`send_to_id`) USING BTREE
) ENGINE=MyISAM DEFAULT CHARSET=utf8;

